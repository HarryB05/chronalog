{
  "name": "chronalog",
  "version": "0.1.2",
  "description": "Git-backed changelog system for Next.js",
  "type": "module",
  "license": "MIT",
  "keywords": [
    "changelog",
    "nextjs",
    "git",
    "mdx",
    "cms",
    "versioning"
  ],
  "main": "dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.js",
      "default": "./dist/client.js"
    },
    "./admin/page": {
      "import": "./dist/admin/page.js",
      "default": "./dist/admin/page.js"
    },
    "./admin/media": {
      "import": "./dist/admin/media.js",
      "default": "./dist/admin/media.js"
    },
    "./admin/components/Editor": {
      "import": "./dist/admin/components/Editor.js",
      "default": "./dist/admin/components/Editor.js"
    },
    "./admin/settings": {
      "import": "./dist/admin/settings.js",
      "default": "./dist/admin/settings.js"
    },
    "./chronalog.css": "./dist/chronalog.css",
    "./admin-page": "./dist/admin/page.js",
    "./admin-media": "./dist/admin/media.js",
    "./admin-settings": "./dist/admin/settings.js"
  },
  "bin": {
    "chronalog": "./dist/cli/init.js"
  },
  "scripts": {
    "build": "tsup && pnpm exec tailwindcss -i ./src/styles/styles.css -o ./dist/chronalog.css",
    "dev": "tsup --watch",
    "lint": "eslint .",
    "prepare": "pnpm build",
    "prepublishOnly": "pnpm build"
  },
  "devDependencies": {
    "@tailwindcss/cli": "^4.0.0",
    "@types/node": "^25.0.3",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@radix-ui/react-popover": "^1.1.15",
    "@tanstack/react-query": "^5.62.0",
    "@tiptap/extension-placeholder": "^3.15.3",
    "@tiptap/react": "^3.15.3",
    "@tiptap/starter-kit": "^3.15.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "jose": "^5.9.0",
    "lucide-react": "^0.468.0",
    "react-day-picker": "^9.13.0",
    "react-markdown": "^9.0.0",
    "tailwind-merge": "^3.4.0"
  },
  "peerDependencies": {
    "next": "^14.0.0 || ^15.0.0 || ^16.0.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "tailwindcss": "^4.0.0"
  }
}
